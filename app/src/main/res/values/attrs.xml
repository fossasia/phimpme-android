<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HelloGallery">
        <attr name="android:galleryItemBackground" />
    </declare-styleable>
    <declare-styleable name="Gallery">
        <attr name="android:galleryItemBackground" />
    </declare-styleable>

    <attr name="IconsColor" format="reference" />
    <attr name="textThemes" format="reference" />
    <attr name="colorPrimaryTheme" format="reference|color" />
    <attr name="BottomNavigationIconsColor" format="reference|color" />
    <attr name="colorPrimaryDarkTheme" format="reference|color" />
    <attr name="colorAccentTheme" format="reference|color" />
    <attr name="windowBackgroundTheme" format="reference|color" />
    <attr name="themeCardColor" format="reference|color" />


    <!--
    WPTextView attributes
-->
    <declare-styleable name="WPTextView">
        <attr name="fixWidowWords" format="boolean"/>
    </declare-styleable>

    <!--
        WPLinearLayoutSizeBound attributes
    -->
    <declare-styleable name="WPLinearLayoutSizeBound">
        <attr name="maxWidth" format="dimension" />
        <attr name="maxHeight" format="dimension" />
    </declare-styleable>

</resources>
